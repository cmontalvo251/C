##First remove links

##Location of C.git from Github
CROOT=../

rm timer.* Datalogger.* MATLAB.* mathp.* nrutils.h

#Then add the links
for VARIABLE in timer.cpp timer.h Datalogger.cpp Datalogger.h MATLAB.cpp MATLAB.h mathp.h mathp.cpp nrutils.h
do
    echo 'Making Link for ' $VARIABLE 
    ln -s ${CROOT}/$VARIABLE $VARIABLE
done
